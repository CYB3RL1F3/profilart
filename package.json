{
  "name": "profilart",
  "version": "1.0.0",
  "description": "Profilart's a nodejs API service that provides aggregated data of electronic music artists from popular APIs / websites (RA, Discogs...), to feed apps, etc...",
  "main": "src/index.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./src/index.js --presets es2015,stage-2",
    "dev": "babel-node ./src/index.js --presets es2015,stage-2 --watch",
    "build": "yarn install && ./node_modules/.bin/babel src --presets es2015,stage-2 --out-dir dist",
    "production": "yarn run build && node ./dist/index.js"
  },
  "author": "Cyberlife <cyberlife.music@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@google/maps": "^0.4.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.19.0",
    "connect-redis": "^3.4.1",
    "cookieparser": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "express-session": "^1.16.2",
    "fs": "^0.0.1-security",
    "graphql": "^14.4.1",
    "html": "^1.0.0",
    "http": "^0.0.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mongo-sanitize": "^1.0.1",
    "mongodb": "3.2.7",
    "node-scrypt": "^0.0.1",
    "node-soundcloud": "^0.0.6",
    "nodemailer": "^6.2.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
