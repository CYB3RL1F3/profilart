{
  "name": "profilart",
  "version": "1.0.0",
  "description": "Profilart's a nodejs websocket service that provides aggregated data of electronic music artists from popular APIs / websites (RA, Discogs...), to feed apps, etc...",
  "main": "src/index.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./src/index.js --presets es2015,stage-2",
    "dev": "babel-node ./src/index.js --presets es2015,stage-2 --watch",
    "build": "yarn install && ./node_modules/.bin/babel src --presets es2015,stage-2 --out-dir dist",
    "production": "yarn run build && node ./dist/index.js"
  },
  "author": "Cyberlife <cyberlife.music@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@google/maps": "^0.4.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.3",
    "connect-redis": "^3.3.3",
    "cookieparser": "^0.1.0",
    "dotenv": "^6.0.0",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "fs": "^0.0.1-security",
    "html": "^1.0.0",
    "http": "^0.0.0",
    "jsonwebtoken": "^8.3.0",
    "mongo-sanitize": "^1.0.0",
    "mongodb": "2.2.33",
    "node-scrypt": "^0.0.1",
    "node-soundcloud": "^0.0.6",
    "nodemailer": "^4.1.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "request": "^2.81.0",
    "scrypt": "^6.0.3",
    "session": "^0.1.0",
    "winston": "^2.3.1",
    "ws": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-watch": "^2.0.7"
  }
}
