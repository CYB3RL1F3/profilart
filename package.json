{
  "name": "profilart",
  "version": "1.1.0",
  "description": "Profilart's a nodejs API service that provides aggregated data of electronic music artists from popular APIs / websites (RA, Discogs...), to feed apps, etc...",
  "main": "src/index.js",
  "engines": {
    "node": "10.13.0",
    "npm": "6.10.0"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon",
    "start:dev": "nodemon",
    "build": "yarn run clean && yarn install && tsc",
    "production": "yarn run build && node ./dist/index.js",
    "postbuild": "yarn run copy-files",
    "copy-files": "./node_modules/copyfiles/copyfiles -u 1 src/**/*.json dist && ./node_modules/copyfiles/copyfiles -u 1 src/**/*.html dist && ./node_modules/copyfiles/copyfiles -u 1 src/**/*.css dist",
    "clean": "rimraf ./dist"
  },
  "author": "Cyberlife <cyberlife.music@gmail.com>",
  "license": "ISC",
  "_moduleAliases": {
    "lib": "dist/lib",
    "config": "dist/config",
    "adapters": "dist/adapters",
    "model": "dist/model",
    "router": "dist/router",
    "services": "dist/services",
    "authenticator": "dist/authenticator",
    "err": "dist/err",
    "gql": "dist/gql",
    "service": "dist/service"
  },
  "dependencies": {
    "@google/maps": "^1.1.3",
    "@sentry/node": "5.27.3",
    "@slack/webhook": "^5.0.3",
    "@types/cheerio": "^0.22.22",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongo-sanitize": "^1.0.1",
    "@types/mongodb": "^3.5.33",
    "@types/node": "^14.14.6",
    "@types/node-cache": "^4.2.5",
    "@types/passport-jwt": "^3.0.3",
    "@types/redis": "^2.8.28",
    "@types/request": "^2.48.5",
    "@types/web-push": "^3.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
